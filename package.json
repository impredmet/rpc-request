{
  "name": "rpc-request",
  "version": "6.0.2",
  "description": "Simple wrapper of the node-fetch library in a class",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vansergen/rpc-request.git"
  },
  "engines": {
    "node": ">=16.9.1",
    "npm": ">=7.21.1"
  },
  "scripts": {
    "build": "tsc",
    "build:remove": "rm -rf dist",
    "ci:all": "npm ci && npm run prettier && npm run eslint && npm run commitlint:all && npm test && npm publish --dry-run && npm run docs:build && npm run coverage",
    "commitlint": "commitlint --verbose",
    "commitlint:all": "npm run commitlint -- --from=3a4e49594ece7f93fd2dab0b8f85a74f83ae1f1c",
    "coverage": "c8 npm test",
    "docs:build": "typedoc",
    "eslint": "eslint --ext .ts ./",
    "eslint:fix": "npm run lint -- --fix",
    "install:clean": "npm install",
    "postbuild": "prettier -c -w dist",
    "postcoverage": "c8 report --reporter=lcov",
    "postdocs:build": "prettier -c -w docs",
    "postpublish": "npm run build:remove",
    "prebuild": "npm run build:remove",
    "preinstall:clean": "rm -rf node_modules package-lock.json",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "prettier": "prettier -c .",
    "prettier:write": "prettier -c -w .",
    "preversion": "npm run docs:build",
    "test": "mocha 'dist/test/**/*.test.js' --full-trace --bail"
  },
  "dependencies": {
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@binden/commitlint-config": "^1.0.0",
    "@binden/semantic-release-config": "^1.1.0",
    "@binden/tsconfig": "^1.0.0",
    "@types/mocha": "^9.1.1",
    "c8": "^7.12.0",
    "eslint-config-binden-ts": "^1.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.10"
  },
  "keywords": [
    "rpc",
    "fetch"
  ],
  "author": "Sergey Bakulin <sergey.bakulin.github@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vansergen/rpc-request/issues"
  },
  "homepage": "https://github.com/vansergen/rpc-request#readme",
  "funding": {
    "type": "Coinbase Commerce",
    "url": "https://commerce.coinbase.com/checkout/eddfb0f7-20e7-4e0d-8e04-232bbdf0b397"
  },
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
